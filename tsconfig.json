{
    "compilerOptions": {
        "module": "commonjs",
        "sourceMap": true,
        "target": "es5",
        "removeComments": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true
    },
    "exclude": [
        "node_modules",
        "wwwroot/lib"
    ]
}
